<% layout('/layouts/boilerplate') %>
<div class="ro">
    <div class="col-8 offset-1 mt-5">
        <div class="alert alert-danger" role="alert">
            <h4 class="alert-heading"><%= err.message %> </h4>
            
            <% if(process.env.NODE_ENV !== 'production') { %> 
            <p class="mb-0"><%= err.stack %> </p>
            <% } %> 
        </div>
    </div>
</div>
